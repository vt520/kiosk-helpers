
#!/bin/bash
source /etc/kiosk/settings
cat << EOF 
KIOSK_MANAGEMENT_USER=$KIOSK_REFLECTOR_USER
KIOSK_MANAGEMENT_IDENTITY=/etc/kiosk/protected/identity
KIOSK_MANAGEMENT_PORT=$(( 2200 + $KIOSK_ID ))
KIOSK_MANAGEMENT_SERVER=$KIOSK_REFLECTOR
KIOSK_MANAGEMENT_SERVER_PORT=22
KIOSK_VNC_PORT=$(( 5900 + $KIOSK_ID ))
KIOSK_HTTP_PORT=$(( 8100 + $KIOSK_ID ))
EOF
