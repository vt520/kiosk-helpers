
#!/bin/bash
[ -e etc/kiosk/setup ] && source etc/kiosk/setup

cat << EOF 
KIOSK_MANAGEMENT_USER=$SETUP_USER
KIOSK_MANAGEMENT_IDENTITY=/etc/kiosk/protected/identity
KIOSK_MANAGEMENT_PORT=$(( 2200 + $SETUP_KIOSK_ID ))
KIOSK_MANAGEMENT_SERVER=$SETUP_KIOSK_SERVER
KIOSK_MANAGEMENT_SERVER_PORT=$SETUP_KIOSK_PORT
KIOSK_VNC_PORT=$(( 5900 + $SETUP_KIOSK_ID ))
KIOSK_HTTP_PORT=$(( 8100 + $SETUP_KIOSK_ID ))
EOF
